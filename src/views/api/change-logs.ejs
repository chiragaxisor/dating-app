    <meta charset="utf-8" />
    <title>Changelogs | <%= process.env.APP_NAME || "Loosh App" %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link
      rel="shortcut icon"
      type="image/x-icon"
      href="<%= process.env.APP_URL  %>/images/favicon.ico"
    />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600&family=Playfair:wght@400;500;600&display=swap"
      rel="stylesheet"
    />
    <style type="text/css">
      body,
      html {
        margin-top: 20px;
        font-family: 'Inter', sans-serif;
      }

      .badge {
        padding: 6px 8px;
        font-family: 'Inter', sans-serif;
      }

      .badge-primary {
        background-color: #dd6b63;
      }

      .text-muted {
        color: #8492a6 !important;
      }

      .bg-light {
        background-color: #add8e624 !important;
      }

      ul {
        list-style-position: outside;
      }

      .text-success {
        color: #0f84ab !important;
      }

      a,
      code {
        color: #dd6b63 !important;
      }

      .container h1,
      .container h4 {
        font-weight: bold;
        color: #dd6b63;
      }

      .display {
        font-family: 'Playfair', serif;
        font-weight: 600;
      }

      code {
        text-align: left;
        font-size: 87.5%;
      }
    </style>
  </head>

